{
  "name": "branchkiller",
  "version": "0.2.3",
  "description": "A command-line tool to delete all git local and remote branches except 'main'. Delete Git branches in bulk.",
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "bin": {
    "branchkiller": "dist/index.js"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "prepare": "npm run build",
    "branchkiller": "npm run build && npm run start",
    "lint": "eslint ."
  },
  "keywords": [
    "git",
    "branch",
    "killer",
    "clear",
    "delete",
    "prune",
    "local",
    "remote"
  ],
  "author": "Sergio Navarro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serNAVARRO7/branchkiller.git"
  },
  "dependencies": {
    "colors": "1.4.0",
    "commander": "^14.0.0",
    "figlet": "^1.8.1",
    "inquirer": "^12.6.0",
    "ora": "^8.2.0",
    "package-json-from-dist": "^1.0.1",
    "simple-git": "^3.27.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/node": "^24.0.3",
    "eslint": "^9.26.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  }
}